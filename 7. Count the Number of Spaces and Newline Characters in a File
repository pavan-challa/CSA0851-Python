def count_spaces_and_newlines(filename):
    space_count = 0
    newline_count = 0
    with open(filename, 'r') as file:
        for line in file:
            space_count += line.count(' ')
            newline_count += 1
    print(f"Spaces: {space_count}")
    print(f"Newlines: {newline_count}")
count_spaces_and_newlines("source.txt")
